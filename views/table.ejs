
<%- include('header') %>

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>#</th>
            <th>Cucc neve</th>
            <th>Cucc típusa</th>
            <th>Kölcsönzés napja</th>
            <th>Kölcsönzött dolog visszahozása</th>
            <th class="text-end">Elérhetőség</th>
        </tr>
    </thead>
    <tbody>
        <% results.forEach((item, index) => { %>
            <tr>    
                <td><%= (index+1) %>.</td>
                <td><%= item.title %></td>
                <td><%= item.type %></td>
                <td><%= item.rental_date %></td>
                <td><%= item.return_date %></td>
                <td class="text-end"><%= item.available %></td>
            </tr>

        <% }); %>
    </tbody>
    <tfoot>
        <tr>
        </tr>
    </tfoot>
</table>

<%- include('footer') %>